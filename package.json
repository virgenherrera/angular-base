{
  "name": "angular-base",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=18.12.0 <19",
    "npm": ">=8.6.0"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "run-s -l check:* build:prod",
    "UT:develop": "ng test --watch --code-coverage --browsers=ChromeHeadless",
    "cleanup": "rimraf coverage/ dist/",
    "check:eslint": "eslint \"src/**/*.ts\"",
    "check:prettier": "prettier --check \"src/**/*.ts\"",
    "check:coverage": "ng test --no-watch --code-coverage --browsers=ChromeHeadless",
    "build:prod": "ng build --optimization --progress --subresource-integrity --delete-output-path",
    "local:ci": "run-s -l check:* build:prod"
  },
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.10",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular/cli": "~14.2.1",
    "@angular/compiler-cli": "^14.2.0",
    "@ngneat/spectator": "^12.2.1",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^4.1.1",
    "typescript": "~4.8.2"
  }
}
